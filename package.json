    "nuxt": "^3.10.0",
    "nuxt-security": "^1.1.0",
    "postcss": "^8.4.33",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@nuxtjs/robots": "^3.0.0"
  }
